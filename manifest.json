{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://avaeduc.com.br/*"
      ],
      "js": [
        "jquery.js",
        "chrome.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":["tabs"]
  /*
  "background":{
        "scripts": ["background.js"]
    }
  */
}